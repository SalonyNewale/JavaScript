//primitive type stored in stack memory gives a copy of value or variable

// 7 types : string, number, boolean, null, undefined, symbol, BigInt

//JavaScript is a dynamic language, meaning that variables can hold different 
//types of values during runtime


//Reference (Non Primitive) type stored in heap memory gives reference of the value

//array, Objects, Functions

const name = ["a", "b"];

let oneObj = {
    name: "Salony",
    age: 22
}

